{
  "http请求": {
    "prefix": "$http",
    "body": [
      "this.\\$http({",
      "  url: this.\\$http.adornUrl(window.SITE_CONFIG.contextPath + '$1'),",
      "  method: '$2',",
      "  data: {$3}",
      "}).then(({ data }) => {",
      "   ",
      "}).catch(() => {",
      "   ",
      "})"
    ],
    "description": "http请求 -- url请求地址| method请求方式(get OR post)| data OR params (请求传参形式，默认data)"
  },
  "放大镜选择事件": {
    "prefix": "dias",
    "body": [
      "selectData (val) {",
      "  if (!val) {",
      "    this.\\$set(this.data, 'Id', null)",
      "  } else if (val) {",
      "    this.\\$set(this.data, 'Id', val.id)",
      "  }",
      "},"
    ],
    "description": "diaSelect(放大镜选择事件)"
  }
}