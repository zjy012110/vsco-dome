{
  "可编辑表格组件": {
    "prefix": "cantab",
    "body": [
      "<canEditTable :tabData=\"dataForm.openCreditEntryList\"",
      "              :tabTagData=\"tabTagName\"",
      "              :tabRules=\"tabRules\"",
      "              :canEdit=\"true\"",
      "              :showHandle=\"true\"",
      "              :showSelection=\"true\"",
      "              ref=\"tabEntry\"",
      "              :getSummaries=\"getSummaries\"",
      "              :summary=\"true\"",
      "              showAdd",
      "              @deleteItem=\"deleteRisk\">",
      "</canEditTable>"
    ],
    "description": "可编辑表格组件"
  },
  "列表搜索栏组件": {
    "prefix": "listFilter",
    "body": [
      "<listFilter @handleSearch=\"handleSearch\"",
      "            @handleReset=\"handleReset\">",
      "  <div slot=\"form\"",
      "       slot-scope=\"props\">",
      "         <el-form class=\"displayFlex\"",
      "                   :model=\"formSearch\"",
      "                   size=\"small\"",
      "                   ref=\"formSearch\">",
      "          <el-row>         ",
      "            <el-col :sm=\"12\"",
      "                    :md=\"12\"",
      "                    :lg=\"8\"",
      "                    :xl=\"8\">",
      "              <el-form-item prop='${1}'>",
      "                <!-- 搜索条件 -->",
      "              </el-form-item>",
      "         </el-col>",
      "      <el-row>",
      "    </el-form>",
      "  </div>",
      "</listFilter>"
    ],
    "description": "列表搜索栏组件"
  },
  "放大镜组件(类型配置)": {
    "prefix": "diaSelectAuto",
    "body": [
      "<diaSelect diaSelectType=\"\"",
      "           v-model=\"\"",
      "           @changeData=\"selectData\"></diaSelect>"
    ],
    "description": "放大镜组件-类型配置"
  },
  "放大镜组件-表格型(手动配置)": {
    "prefix": "diaSelectTable",
    "body": [
      "<diaSelect data-type=\"table\"",
      "           v-model=\"\"",
      "           is-search",
      "           url=\"\"",
      "           :urlData=\"{}\"",
      "           :onlySelect=\"false\"",
      "           @changeData=\"selectData\"",
      "           :tabTagData=\"tableData\"></diaSelect>"
    ],
    "description": "放大镜组件-表格型(手动配置)"
  },
  "放大镜组件-树型(手动配置)": {
    "prefix": "diaSelectTree",
    "body": [
      "<diaSelect data-type=\"tree\"",
      "           v-model=\"\"",
      "           is-search",
      "           url=\"\"",
      "           :urlData=\"{}\"",
      "           :dbClickFlag=\"false\"",
      "           :onlySelect=\"false\"",
      "           :defaultProps=\"{",
      "                   children: 'children',",
      "                   label: 'name'",
      "            }\"",
      "            @changeData=\"selectData\"></diaSelect>",
      ""
    ],
    "description": "放大镜组件-树型(手动配置)"
  }
}