{
  "可编辑表格组件": {
    "prefix": "canEditTable",
    "body": [
      "<canEditTable :tabData=\"dataForm.openCreditEntryList\"",
      "              :tabTagData=\"tabTagName\"",
      "              :tabRules=\"tabRules\"",
      "              :canEdit=\"true\"",
      "              :showHandle=\"true\"",
      "              :showSelection=\"true\"",
      "              ref=\"tabEntry\"",
      "              :getSummaries=\"getSummaries\"",
      "              :summary=\"true\"",
      "              showAdd",
      "              @deleteItem=\"deleteRisk\">",
      "</canEditTable>"
    ],
    "description": "可编辑表格组件"
  },
  "列表搜索栏组件": {
    "prefix": "listFilter",
    "body": [
      "<listFilter @handleSearch=\"handleSearch\"",
      "            @handleReset=\"handleReset\">",
      "  <div slot=\"form\"",
      "       slot-scope=\"props\">",
      "         <el-form class=\"displayFlex\"",
      "                   :model=\"formSearch\"",
      "                   size=\"small\"",
      "                   ref=\"formSearch\">",
      "          <el-row>         ",
      "            <el-col :sm=\"12\"",
      "                    :md=\"12\"",
      "                    :lg=\"8\"",
      "                    :xl=\"8\">",
      "              <el-form-item prop='${1}'>",
      "                <!-- 搜索条件 -->",
      "              </el-form-item>",
      "         </el-col>",
      "      <el-row>",
      "    </el-form>",
      "  </div>",
      "</listFilter>"
    ],
    "description": "列表搜索栏组件"
  },
  "放大镜组件(类型配置)": {
    "prefix": "diaSelectAuto",
    "body": [
      "<diaSelect diaSelectType=\"\"",
      "           v-model=\"\"",
      "           cueString=\"\"",
      "           @changeData=\"selectData\"></diaSelect>"
    ],
    "description": "放大镜组件-类型配置(diaSelectType传入放大镜类型，如：customer)"
  },
  "放大镜组件-表格型(手动配置)": {
    "prefix": "diaSelectTable",
    "body": [
      "<diaSelect data-type=\"table\"",
      "           v-model=\"\"",
      "           is-search",
      "           url=\"\"",
      "           :urlData=\"{}\"",
      "           :onlySelect=\"false\"",
      "           cueString=\"\"",
      "           @changeData=\"selectData\"",
      "           :tabTagData=\"tableData\"></diaSelect>"
    ],
    "description": "放大镜组件-表格型(手动配置)"
  },
  "放大镜组件-树型(手动配置)": {
    "prefix": "diaSelectTree",
    "body": [
      "<diaSelect data-type=\"tree\"",
      "           v-model=\"\"",
      "           is-search",
      "           url=\"\"",
      "           :urlData=\"{}\"",
      "           :dbClickFlag=\"false\"",
      "           :onlySelect=\"false\"",
      "           :defaultProps=\"{",
      "                   children: 'children',",
      "                   label: 'name'",
      "            }\"",
      "           cueString=\"\"",
      "           @changeData=\"selectData\"></diaSelect>",
      ""
    ],
    "description": "放大镜组件-树型(手动配置)"
  },
  "数值组件-小数位,千分位控制": {
    "prefix": "numeric",
    "body": [
      "<numeric dataType='amount'",
      "         v-model=\"\"></numeric>",
      ""
    ],
    "description": "数值组件-小数位,千分位控制: dataType 数值类型：(数量：qty   金额：amount  汇率：exchange  税率：taxRate 单价：price)"
  },
  "列表表格组件listCommon": {
    "prefix": "listCommon",
    "body": [
      "<listCommon :fn=\"add\"",
      "            :showEdit=\"true\"",
      "            :table-list=\"tariffsArray\"",
      "            :columnList=\"columnArray\"",
      "            :show-selection=\"true\"",
      "            :isExport='false'",
      "            :dataListLoading=\"loadState\"",
      "            :show-order=\"true\"",
      "            :showBlank=\"false\"",
      "           :handleSelectionChange=\"handleSelectionChange\">",
      "   <template slot=\"tabButton\">",
      "        <el-button type=\"primary\"",
      "                   size=\"mini\"",
      "                   @click=\"addPage\">新增</el-button>",
      "   </template>",
      "</listCommon>"
    ],
    "description": "列表表格组件listCommon"
  }
}